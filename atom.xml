<span class="tag">&lt;<span class="title">div</span> <span class="attribute">id</span>=<span class="value">"i4-test"</span> <span class="attribute">class</span>=<span class="value">"class04"</span>&gt;</span><span class="tag">&lt;/<span class="title">div</span>&gt;</span>
<span class="tag">&lt;<span class="title">div</span> <span class="attribute">id</span>=<span class="value">"i5-test"</span> <span class="attribute">class</span>=<span class="value">"class05"</span>&gt;</span><span class="tag">&lt;/<span class="title">div</span>&gt;</span>
</pre></td></tr></table></figure>

<p>div#header&gt;ul#navigation&gt;li.item$*5&gt;a</p>
